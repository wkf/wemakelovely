@import '_settings'
@import '_fonts'

normalize()
base()

*
  margin: 0
  padding: 0

img
  outline: none
  border: none

.contents
  position: absolute
  top: 0
  bottom: 0
  left: 0
  right: 0
  margin: auto
  height: 147px
  width: 100px

.social-icon
  size: 40 40
  padding: 30px 53px

.our-email
  @extends .social-icon
  position: absolute
  left: 0px
  top: 0px
  z-index: -1
  opacity: 0

.logo
  height: 100px

a:hover .logo
  opacity: .5

a:hover .our-email
  opacity: 1